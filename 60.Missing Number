//https://leetcode.com/problems/missing-number/submissions/
class Solution {
    public int missingNumber(int[] nums) {
        Arrays.sort(nums);
        if(nums.length==nums[nums.length-1])
        {
            for(int i=0;i<nums.length;i++)
            {
                if(nums[i]!=i)
                    return i;
            }
        }
        else
            return nums.length;
        
        return 0;
        
    }
}
